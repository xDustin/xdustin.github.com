var myName = prompt("Please enter your first name") ;
if (myName.length <= 7) {
    console.log("I finished my first course!");
    console.log("I have a short name:");
    console.log(myName);
}
else {
    console.log("I finished my first course!");
    console.log("I have a long name:");
    console.log(myName);
}
